include ../header/header
include ../footer/footer

mixin ui-kit-page4
  +b.ui-kit-page4
    +e.container
      - const headerConfig = require('../../modules/ui-kit-page4/config/header.json');
      +header(headerConfig)

    +e.container
      - const headerUserConfig = require('../../modules/ui-kit-page4/config/header-user.json');
      +header(headerUserConfig)

    +e.container
      +footer

    +e.footer-minimal
      +footer._minimal
